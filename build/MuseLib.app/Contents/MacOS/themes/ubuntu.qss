QWidget#centralwidget {
border-image:url(themes/sheet-music-texture.png) 0 0 stretch stretch;
}

QGraphicsView#scoreWindow {
background:transparent;
}

QWidget#searchBar {
background:black;

}

QLabel#titleOfPiece {
    color:white;
}

QFrame#searchFrame {
    border:0px;
}

QTableWidget {
    border:0px;
}
QTreeWidget#searchTree {
background:#F3F3F3;
    border:0px;
}

QTreeWidget QHeaderView {
background:transparent;
}
QHeaderView:section {
background:rgba(255,255,255,150);
border:0px;
padding:5px;
font: 14pt "Open Sans";
color:#736F66;
}

QFrame#contentFrame {
border:0px;
background:#A0E8D6;
border-left:1px solid rgb(137, 216, 194);
}

QWidget#scorebookWidget {
background:#eee;
border:1px solid #ddd;
}

QWidget#scorebookWidget QListWidget {
background:rgba(255, 255, 255, 255);
border:0px;
}

QWidget#scorebookWidget QLabel {
color:#B14623;
background:#602749;
border-bottom:1px solid #999;
padding-top:10px;
}

QWidget#searchBar QLineEdit {
border-radius: 10px;
    font:15pt "Open Sans";
}

QFrame#buttonFrame {
background:#3E1C33;
}

QFrame QToolButton, QWidget QToolButton {
background:transparent;
padding:10px;
border:0px;
color:white;
}

QFrame QToolButton:pressed {
    background-color:black;
}

QFrame QToolButton#autoPlaylistBtn {
background-image: url(themes/icons/autoplaylist.png);
    background-position: center;
    background-repeat: no-repeat;
}

QFrame QToolButton#myPlaylistBtn {
background-image: url(themes/icons/myplaylist.png);
    background-position: center;
    background-repeat: no-repeat;
}

QFrame QToolButton#infoBtn {
background-image: url(themes/icons/info.png);
    background-position: center;
    background-repeat: no-repeat;
}

QFrame QToolButton#scorebookBtn {
background-image: url(themes/icons/scorebook.png);
    background-position: center;
    background-repeat: no-repeat;
}

QFrame QToolButton#featuredBtn {
background-image: url(themes/icons/featured.png);
    background-position: center;
    background-repeat: no-repeat;
}

QFrame QToolButton#browserBtn {
background-image: url(themes/icons/browser.png);
    background-position: center;
    background-repeat: no-repeat;
}

QWidget QToolButton#multistndBtn {
    background-image: url(themes/icons/multistand.png);
    background-position: center;
    background-repeat: no-repeat;
}

QWidget QToolButton#popoutBtn
{
    background-image: url(themes/icons/popout.png);
    background-position: center;
    background-repeat: no-repeat;
}

QToolButton#zoomInBtn {
    background: url(themes/icons/zoom-in.png) center no-repeat;
}

QToolButton#zoomOutBtn {
    background: url(themes/icons/zoom-out.png) center no-repeat;
}